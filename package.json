{
  "name": "any-llm",
  "version": "0.1.22",
  "description": "A seamless typescript abstraction layer between your application and various Language Learning Model (LLM) providers.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fkesheh/any-llm.git"
  },
  "bugs": {
    "url": "https://github.com/fkesheh/any-llm/issues"
  },
  "homepage": "https://github.com/fkesheh/any-llm#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm dist -rf && tsc && tsc-alias",
    "prettier": "prettier --write src",
    "lint": "eslint . --ext .ts",
    "prePublish": "npm run build && npm run lint && npm run prettier",
    "npmPublish": "npm run prePublish && npm publish --access=public"
  },
  "keywords": [
    "abstraction-layer",
    "LLM",
    "language-learning-model"
  ],
  "author": "Foad Kesheh",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.3"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@util/*": [
        "src/util/*"
      ],
      "@chatModels/*": [
        "src/chatModels/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@implementations/*": [
        "src/impl/*"
      ],
      "@embeddings/*": [
        "src/embeddings/*"
      ]
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@google/generative-ai": "^0.3.1",
    "@lenml/llama2-tokenizer": "^1.1.3",
    "@lenml/llama2-tokenizer-vocab-llama2": "^1.1.1",
    "ai": "^3.0.13",
    "axios": "^1.6.8",
    "cohere-ai": "^7.9.0",
    "dotenv": "^16.4.5",
    "openai": "^4.29.2",
    "tiktoken": "^1.0.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  }
}
