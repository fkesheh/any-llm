{
  "name": "any-llm",
  "version": "0.1.7",
  "description": "A seamless typescript abstraction layer between your application and various Language Learning Model (LLM) providers.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fkesheh/any-llm.git"
  },
  "bugs": {
    "url": "https://github.com/fkesheh/any-llm/issues"
  },
  "homepage": "https://github.com/fkesheh/any-llm#readme",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc --noEmit && babel src --out-dir lib --extensions \".ts,.tsx\"",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prePublish": "npm run build && npm run lint && npm run prettier",
    "publish": "npm run prePublish && npm publish --access=public"
  },
  "keywords": [
    "abstraction-layer",
    "LLM",
    "language-learning-model"
  ],
  "author": "Foad Kesheh",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@util/*": [
        "src/util/*"
      ],
      "@llm/*": [
        "src/llm/*"
      ]
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.19.0",
    "@google/generative-ai": "^0.3.1",
    "ai": "^3.0.13",
    "cohere-ai": "^7.9.0",
    "dotenv": "^16.4.5",
    "openai": "^4.29.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  }
}
